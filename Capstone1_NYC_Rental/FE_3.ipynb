    "import re\n",
    "\n",
    "def uniq_term(df, name):\n",
    "    terms = set()\n",
    "    for x in df['features']:\n",
    "        for phrase in x:\n",
    "            phrase_lc = phrase.strip().lower()\n",
    "            if re.search(name, phrase_lc):\n",
    "                terms.add(phrase_lc)\n",
    "        \n",
    "    unique_terms = sorted(terms)\n",
    "    for term in unique_terms:\n",
    "        print(term)\n",
    "        \n",
    "uniq_term(combined_df, 'laundry')"
